<% if @likeable.is_a? Question %>
  $("#<%=Question%><%=@likeable.id%> .VoteButton").html("<%= j (render 'add_like', path: question_likes_path(@likeable, Like.new)) %>");
<%else%>
  $("#<%=Answer%><%=@likeable.id%> .VoteButton").html("<%= j (render 'add_like', path: answer_likes_path(@likeable, Like.new)) %>");
<%end%>
$("#<%=@like.likeable_type%><%=@likeable.id%> .NumVotes").html(parseInt($("#<%=@like.likeable_type%><%=@likeable.id%> .NumVotes").html(), 10)-1);
