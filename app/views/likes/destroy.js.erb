<% if @likeable.is_a? Question %>
  $("#<%=Question%><%=@question.id%> .Vote").html("<%= j (render 'like', likeable: @likeable, path: question_likes_path(@likeable, Like.new)) %>");
<%else%>
  $("#<%=Answer%><%=@answer.id%> .Vote").html("<%= j (render 'like', likeable: @likeable, path: answer_likes_path(@likeable, Like.new)) %>");
<%end%>
